{
    "manifest_version": 3,
    "name": "인사이트",
    "description": "보이는 모든것을 읽어드립니다",
    "version": "1.0",
    "action": { "default_popup": "popup.html"},
    "background": {
      "service_worker": "static/background.js"
    },
    "permissions": ["storage"],
      "content_scripts": [
        {
          "matches": ["<all_urls>"],
          "js": ["content.js"],
          "run_at": "document_idle"
        }
      ]
     
}
